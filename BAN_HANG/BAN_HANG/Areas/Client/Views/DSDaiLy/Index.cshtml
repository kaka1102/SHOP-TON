@using CS.Data.DataContext

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Client/Views/Shared/_Layout.cshtml";

    var data = (List<Branch>) Model;
}

<div class="container">
        
    <div style="padding-bottom: 40px;padding-top: 50px">
        @foreach (var item in data)
        {
            <div class="col-md-4 footer-in" style="padding-bottom: 15px">
                <h4 style="text-transform: uppercase"><span class=""> @item.Name </span></h4>
                <p>@item.AddressName</p>
            </div>
        }
        <div class="clearfix"></div>
    </div>
</div>